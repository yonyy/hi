{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "experimentalObjectRestSpread": true,
    "defaultParams": true,
    "destructuring": true,
    "spread": true,
    "restParams": true
  }
}